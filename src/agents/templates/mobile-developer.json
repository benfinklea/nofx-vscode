    "commands": {
        "development": {
            "ios": {
                "run": "npx react-native run-ios",
                "build": "xcodebuild -workspace ios/*.xcworkspace -scheme {scheme} -configuration {config}",
                "simulator": "xcrun simctl boot {device_id}",
                "clean": "cd ios && xcodebuild clean && pod install"
            },
            "android": {
                "run": "npx react-native run-android",
                "build": "./gradlew assembleRelease",
                "emulator": "emulator -avd {device_name}",
                "clean": "cd android && ./gradlew clean"
            },
            "flutter": {
                "run": "flutter run",
                "build:ios": "flutter build ios",
                "build:android": "flutter build apk",
                "clean": "flutter clean"
            }
        },
        "testing": {
            "unit": "npm test",
            "integration": "detox test",
            "e2e:ios": "detox test --configuration ios",
            "e2e:android": "detox test --configuration android",
            "coverage": "npm run test:coverage"
        },
        "deployment": {
            "build:production": "eas build --platform all",
            "submit:ios": "eas submit --platform ios",
            "submit:android": "eas submit --platform android",
            "codepush": "appcenter codepush release-react",
            "fastlane:ios": "fastlane ios release",
            "fastlane:android": "fastlane android release"
        },
        "debugging": {
            "metro": "npx react-native start",
            "flipper": "open -a Flipper",
            "devmenu:ios": "xcrun simctl openurl booted {deep_link}",
            "devmenu:android": "adb shell input keyevent 82",
            "logs:ios": "xcrun simctl spawn booted log stream --level debug",
            "logs:android": "adb logcat"
        }
    },

